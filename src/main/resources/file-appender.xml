<included>
    <appender name="FILE" class="ch.qos.logback.core.FileAppender">
        <file>./logs/file.log</file>
        <encoder>
            <pattern>[%d{yyyy-MM-dd'T'HH:mm:ss.SSSXXX}] [%-5level] [%-36.36X{REQUEST_ID:-REQUEST_ID}] --- [%-15.15thread] %-40.40logger{36} : %msg%n</pattern>
            <charset>utf8</charset>
        </encoder>
    </appender>
</included>
